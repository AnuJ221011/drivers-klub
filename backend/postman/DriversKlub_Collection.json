                {
                    "name": "Get Available Rental Plans",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "var jsonData = pm.response.json();",
                                    "if (jsonData.success && jsonData.data.length > 0) {",
                                    "    pm.environment.set(\"rentalPlanId\", jsonData.data[0].id);",
